pipeline{
  agent any
  
  stages{
    stage('Checkout') {
     steps{
        git credentialsId: '', url: 'https://github.com/Nandhini-1999/New_project.git'
      }
     }
  stage('Build'){
      steps{
        bat "mvn install"
      }
    }
    
    stage('Test'){
      steps{
        bat "mvn test"
      }
    }
    stage('Package'){
      steps{
        bat "mvn package"
      }
    }
    }
    }
